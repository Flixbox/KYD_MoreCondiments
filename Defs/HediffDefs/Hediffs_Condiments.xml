<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef>
		<defName>KYD_ConsumedRoyalJelly</defName>
		<label>consumed royal insect jelly preserves (from meal)</label>
		<makesSickThought>false</makesSickThought>
		<description>Royal insect jelly preserves are smooth, rich, and boosts immunity when consumed. Gives a massive 30% immunity gain speed boost, but only for 6 hours.</description>
		<tendable>false</tendable>
		<isBad>false</isBad>
		<defaultLabelColor>(92,62,198)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<comps>
			<li Class="VanillaCookingExpanded.HediffCompProperties_WhileHavingThoughts">
				<thoughtDefs>
					<li>KYD_ConsumedRoyalJelly</li>
				</thoughtDefs>
			</li>
		</comps>
		<stages>
			<li>   
				<statOffsets>
					<ImmunityGainSpeed>0.30</ImmunityGainSpeed>					
				</statOffsets>
			</li>
		</stages>
	</HediffDef>
</Defs>